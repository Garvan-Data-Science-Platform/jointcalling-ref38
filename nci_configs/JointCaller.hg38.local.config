        clusterOptions = '-S /bin/bash -l mem=80000M -l jobfs=15625M -l storage=scratch/np30+gdata/np30'
      }
      withName: SNPsVariantRecalibratorClassic {
        clusterOptions = '-S /bin/bash -l mem=80000M -l jobfs=15625M -l storage=scratch/np30+gdata/np30'
      }
      withName: ApplyRecalibraton {
        cpus = '2'
	runOptions = '--bind "/proc"'
